{
  "name": "@ft/webrtc",
  "version": "1.0.0",
  "scripts": {
    "dev:fe": "cross-env NODE_ENV=development rspack build -c rspack.client.js --watch",
    "build:fe": "rspack build -c rspack.client.js",
    "server": "mkdir -p build && rollup -c rollup.server.js && node build/server.js",
    "dev:server": "nodemon --watch server --ext ts --exec \"npm run server\"",
    "build:server": "mkdir -p build && rollup -c rollup.server.js",
    "dev": "concurrently \"npm run dev:fe\" \"npm run dev:server\"",
    "build": "rimraf build && npm run build:fe && npm run build:server",
    "deploy": "npm run build && node build/server.js",
    "build:deploy": "npm run build && pnpm -F . --prod deploy ../../output",
    "lint:ts": "../../node_modules/typescript/bin/tsc --noEmit -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WindrunnerMax/FileTransfer.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WindrunnerMax/FileTransfer/issues"
  },
  "homepage": "https://github.com/WindrunnerMax/FileTransfer",
  "dependencies": {
    "@arco-design/web-react": "2.56.1",
    "laser-utils": "0.0.5-alpha.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "4.7.2",
    "socket.io-client": "4.7.2",
    "express": "4.18.2",
    "vconsole": "^3.15.1"
  },
  "devDependencies": {
    "@rspack/cli": "0.2.5",
    "@rspack/plugin-html": "0.2.5",
    "@types/express": "4.17.21",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "concurrently": "8.2.2",
    "copy-webpack-plugin": "5",
    "cross-env": "7.0.3",
    "esbuild": "0.19.6",
    "less": "3.0.0",
    "less-loader": "6.0.0",
    "postcss": "8.3.3",
    "prettier": "2.4.1",
    "rimraf": "6.0.1",
    "rollup": "2.75.7",
    "rollup-plugin-esbuild": "6.1.0",
    "sass": "1.52.3",
    "nodemon": "3.0.1",
    "sass-loader": "13.3.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.11.0"
  }
}
